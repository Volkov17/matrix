#include <iostream>
using namespace std;


int main (){
  int n,m ;
   cout<< "vvedite n,m"<<endl;
   cin >>n>>m;#include <iostream>
using namespace std;


int main (){
  int n,m ;
   cout<< "vvedite n,m"<<endl;
   cin >>n>>m;
   int a[n][m],b[n][m],c[n][m];
   for (int i=0 ;i<n;i++){
     for (int j=0;j< m;j++){
        cin>>a[i][j];
     }
   }
   for (int i=0 ;i<n;i++){
     for (int j=0;j< m;j++){
        cin>>b[i][j];
     }
   }

   for (int i=0 ;i<n;i++){
     for (int j=0;j< m;j++){
       c[i][j]=a[i][j]+b[i][j];
       cout<<c[i][j]<<" ";
     }
     cout<<endl;
   }
return 0;
}


   int a[n][m],b[n][m],c[n][m];
   for (int i=0 ;i<n;i++){
     for (int j=0;j< m;j++){
        cin>>a[i][j];
     }
   }
   for (int i=0 ;i<n;i++){
     for (int j=0;j< m;j++){
        cin>>b[i][j];
     }
   }

